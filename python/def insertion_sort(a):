def insertion_sort(a):
    for i in range(1, len(a)):
        tmp = a[i]
        j = i - 1
        while j >= 0 and a[j]>tmp:
            a[j + 1] = a[j]
            j -= 1
        if ((j + 1) != i):
            a[j + 1] = tmp
            print(*arr)

nn = int(input())
arr = list(map(int, input().split()))
insertion_sort(arr)